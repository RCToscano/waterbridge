<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>br.com.waterbridge.bo.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HomeBO</servlet-name>
    <servlet-class>br.com.waterbridge.bo.HomeBO</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GraficoConsumoBO</servlet-name>
    <servlet-class>br.com.waterbridge.bo.GraficoConsumoBO</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MessageBO</servlet-name>
    <servlet-class>br.com.waterbridge.bo.MessageBO</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CadastroBO</servlet-name>
    <servlet-class>br.com.waterbridge.bo.CadastroBO</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomeBO</servlet-name>
    <url-pattern>/HomeBO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GraficoConsumoBO</servlet-name>
    <url-pattern>/GraficoConsumoBO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MessageBO</servlet-name>
    <url-pattern>/MessageBO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CadastroBO</servlet-name>
    <url-pattern>/CadastroBO</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>